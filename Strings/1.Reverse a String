#include<bits/stdc++.h>
using namespace std;


void reverse(string &str)
{
    int n = str.length();
    
    for(int i=0;i<n/2;i++)
    {
        swap(str[i],str[n-i-1]);
    }
}

void print(string &str){
    int n = str.length();
    for(int i=0;i<n;i++)
    {
        cout<<str[i];
    }
}
int main(){
    string str ;
    getline(cin,str);
    reverse(str);
    print(str);
    return 0;
    
}
